<div class="title">
  <p>Menus</p>
</div>
<div class="links">
  <% @menus.each do |menu| %>
    <p><%= link_to "#{menu.bar_name.name}", menu_path(menu) %></p>
  <% end %>
  <div class="link-box">
    <%= link_to "New Menu", menus_path, method: :post %>
  </div>
</div>
<i class="material-icons">expand_more</i>
